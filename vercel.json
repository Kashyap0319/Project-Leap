{
  "buildCommand": "cd frontend/dashboard && npm install && npm run build",
  "outputDirectory": "frontend/dashboard/.next",
  "devCommand": "cd frontend/dashboard && npm run dev",
  "installCommand": "cd frontend/dashboard && npm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://your-backend-url.com/api/:path*"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_BASE_URL": "https://your-backend-url.com"
  }
}

